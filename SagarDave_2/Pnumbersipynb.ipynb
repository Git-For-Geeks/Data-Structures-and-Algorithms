      "source": [
        "n = int(input())\n",
        "a = list(map(int, input().split()))\n",
        "c = 0\n",
        "while a:\n",
        "    mn = min(a)\n",
        "    a = [i for i in a if i % mn != 0]\n",
        "    c += 1\n",
        " \n",
        "print(c)"
      ]
